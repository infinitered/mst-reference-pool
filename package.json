{
  "name": "mst-reference-pool",
  "version": "0.1.0",
  "description": "MobX-State-Tree extension to create reference pools",
  "main": "build/index.js",
  "repository": "https://github.com/infinitered/mst-reference-pool",
  "author": "Jamon Holmgren",
  "license": "MIT",
  "private": false,
  "files": [
    "build"
  ],
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "test": "jest",
    "ci:publish": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.0",
    "@types/jest": "^27.0.2",
    "jest": "^27.3.1",
    "mobx": "^6.3.5",
    "mobx-state-tree": "^5.0.4",
    "semantic-release": "^18.0.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "mobx": ">=4.0.0",
    "mobx-state-tree": ">=3.0.0"
  },
  "prettier": {
    "semi": false
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": "package.json",
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
